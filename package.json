    "chai": "^3.5.0",
    "flow-bin": "^0.42.0",
    "imagemagick": "^0.1.3",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "react-test-renderer": "~15.4.0"
  },
  "jest": {
    "preset": "react-native",
